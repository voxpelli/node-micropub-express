    "eslint-plugin-promise": "3.8.0",
    "eslint-plugin-standard": "2.3.1",
    "husky": "0.13.4",
    "installed-check": "2.1.3",
    "mocha": "3.5.3",
    "nock": "9.3.0",
    "nyc": "^12.0.2",
    "sinon": "1.17.7",
